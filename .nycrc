{
    "include": [
        "src/main/**/*.ts"
    ],
    "extension": [
        ".ts",
        ".tsx"
    ],
    "exclude": [
        "**/index.ts",
        "**/*.d.ts"
    ],
    "require": [
        "jsdom-global/register",
        "ts-node/register",
        "tsconfig-paths/register",
        "source-map-support/register"
    ],
    "reporter": [
        "text"
    ],
    "all": true
}